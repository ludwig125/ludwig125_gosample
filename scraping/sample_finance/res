<script type="text/javascript">

$(function(){
    $('.js-drawerMenuBtn').click(function(e){
        e.stopPropagation();
        var $menu = $('.m-drawerMenu');
        var $main = $('.m-drawerMenuMain_inner');
        var $fiexdElm_L = $('.m-drawerMenuMain_fixedLeftElm');
        var $fiexdElm_R = $('.m-drawerMenuMain_fixedRightElm');
        if($(this).hasClass('open')){
            $(this).removeClass('open').animate({'left' : '-=250'}, 300, 'swing');
            $menu.css({'z-index' : '-1'});
            $main.removeClass('open').animate({'margin-left' : '0', 'margin-right' : '0'}, 300, 'swing', function(){
                $menu.css({'visibility' : 'hidden'});
            });
            $fiexdElm_L.animate({'left' : '-=250'}, 300, 'swing');
            $fiexdElm_R.animate({'right' : '+=250'}, 300, 'swing');

        }else{
            $(this).addClass('open').animate({'left' : '+=250'}, 300, 'swing');
            $menu.css({'visibility' : 'visible'});
            $main.addClass('open').animate({'margin-left' : '250px', 'margin-right' : '-250px'}, 300, 'swing', function(){
                $menu.css({'z-index' : '1'});
            });
            $fiexdElm_L.animate({'left' : '+=250'}, 300, 'swing');
            $fiexdElm_R.animate({'right' : '-=250'}, 300, 'swing');
        }
    });
    $('.js-drawerMenuMain').click(function(){
        var $btn = $('.js-drawerMenuBtn');
        var $menu = $('.m-drawerMenu');
        var $fiexdElm_L = $('.m-drawerMenuMain_fixedLeftElm');
        var $fiexdElm_R = $('.m-drawerMenuMain_fixedRightElm');
        if($(this).hasClass('open')){
            $btn.removeClass('open').animate({'left' : '-=250'}, 300, 'swing');
            $menu.css({'z-index' : '-1'});
            $(this).removeClass('open').animate({'margin-left' : '0', 'margin-right' : '0'}, 300, 'swing', function(){
                $menu.css({'visibility' : 'hidden'});
            });
            $fiexdElm_L.animate({'left' : '-=250'}, 300, 'swing');
            $fiexdElm_R.animate({'right' : '+=250'}, 300, 'swing');
        }
    });

    adjustDrawerMenu();
    $(window).resize(function(){
        var timer = false;
        if (timer !== false) {
            clearTimeout(timer);
        }
        timer = setTimeout(function() {
            adjustDrawerMenu();
        }, 200);
    });
    function adjustDrawerMenu(){
        var winh = $(window).height();
        $('.m-drawerMenu_body').height((winh - $('.m-drawerMenu_header').outerHeight()));
    }

    
    var wideLayoutMql = window.matchMedia("(min-width:640px)");
    wideLayoutMql.addListener(handleWideLayout);
    handleWideLayout(wideLayoutMql);
    function handleWideLayout(mql) {
        if (mql.matches) {
            var $btn = $('.js-drawerMenuBtn');
            var $menu = $('.m-drawerMenu');
            var $main = $('.m-drawerMenuMain_inner');
            var $fiexdElm_L = $('.m-drawerMenuMain_fixedLeftElm');
            var $fiexdElm_R = $('.m-drawerMenuMain_fixedRightElm');
            if($main.hasClass('open')){
                $btn.removeClass('open').css({'left' : ''});
                $menu.css({'z-index' : '', 'visibility' : ''});
                $main.removeClass('open').css({'margin-left' : '', 'margin-right' : ''});
                $fiexdElm_L.css({'left' : ''});
                $fiexdElm_R.css({'right' : ''});
            }

        }
    }

});
</script>
