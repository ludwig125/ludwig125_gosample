<script>
var ua = navigator.userAgent;
var is_sp = (ua.indexOf('iPhone') > 0 || ua.indexOf('iPod') > 0 || ua.indexOf('iPad') > 0 || ua.indexOf('Android') > 0);
if(is_sp) {
    var $naviLink = $('.l-miH03_item > a');
    window.hrefList = [];
    $naviLink.each(function() {
        window.hrefList.push($(this).attr('href'));
        $(this)
            .attr('href', 'javascript:void(0)');
    });
    $('.l-miH04B_listItem > a').each(function() {
       var $innerLink = $(this);
       $innerLink
           .css('display', 'block');
       if($innerLink.attr('target')) {
           $innerLink
               .removeAttr('target');
       }
    });
    var clickCnt = 0, targetIndexTmp = -1, menuHeight = 0;
    $naviLink.click(function() {
        var $this = $(this);
        $naviLink.each(function() {
            $(this)
                .css({'color': '#fff', 'background-color': '#0a385b'})
                .parent()
                .removeClass('menu-active')
                .find('.l-miH03_menu')
                .css({'opacity': 0, 'overflow': 'hidden'});
        });
        if(targetIndexTmp !== $naviLink.index(this)) {
            clickCnt = 0;
        }
        targetIndexTmp = $naviLink.index(this);
        $this
            .data('click', ++clickCnt)
            .css({'color': '#0a385b', 'background-color': '#fff'})
            .parent()
            .addClass('menu-active')
            .find('.l-miH03_menu')
            .css({'opacity': 1, 'overflow': 'initial'});
        if($this.data('click') === 2 || $naviLink.index(this) === 11) {
            $this.data('click', 0);
            location.href = window.hrefList[$naviLink.index(this)];
        } else if($('.l-proTimeline_inner').length > 0) {
            $('body')
                .css('zoom', 1.001);
            var ti = setInterval(function() {
                $('body')
                    .css('zoom', 1);
                clearInterval(ti);
            }, 20);
        }
    });
} else {
    $('.l-miH03_item').filter('.noMenu').hover(function(){ $(this).addClass('menu-active'); }, function(){ $(this).removeClass('menu-active'); });
    var $menu = $('.l-miH03_item').not('.noMenu');
    var $menu_hovered; // 現在hoverされてるメニュー
    $menu.mouseenter(function(e){
        var $this = $(this);
        $menu_hovered = $this;
        if($this.data('leavetimer')){
            clearInterval($this.data('leavetimer'));
            $this.data('leavetimer', null);
        }
        if($menu.hasClass('menu-active') == false){
            $this.addClass('menu-active');
        }
    }).mouseleave(function(e){
        var $this = $(this);
        $menu_hovered = null;
        $this.data('leavetimer', setTimeout(function(obj){ obj.removeClass('menu-active'); if($menu_hovered) $menu_hovered.trigger('mouseenter'); }, 200, $this));
    });
}
</script>
