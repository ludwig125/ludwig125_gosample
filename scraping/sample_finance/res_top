_nk.getSvgGraph = function(sTargetId, targetURL, oOpt)
{

    var oNk = _nk;
    var $Base = $(sTargetId);
    for(key in oOpt){
        targetURL = targetURL + "&" + key + "=" +  oOpt[key];
    }
    
    if (window.matchMedia != undefined && window.matchMedia('screen and (max-width:639px)').matches) {
        
        targetURL = targetURL + "&" + "chart_style=s1";
    }

    $Base.html('<img src=\"//assets.nikkei.jp/release/v3.1.38/parts/ds/images/nkd/common/img_loading.svg\" data-original=\"' + targetURL + '\">');

    $('#'+sTargetId+' img').lazyload();

    $Base.children('img').error(function(){
        if ($Base.parent().is('a')) {
            
            var divTargetId = $Base.selector.replace("#","");
            $Base.parent('a').replaceWith('<div id="' + divTargetId + '" class="m-figure_inner">現在、描画できません。</div>');
        }
        else {
            
            $Base.html("現在、描画できません。");
        }
    });

    return;
};
</script>

