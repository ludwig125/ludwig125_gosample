<script type="text/javascript">
_nk.stockInfoBallonBaseOver = function (sTargetId) {
    var oNk = _nk;
    var $Target = $(sTargetId);

    $Target.bind('mouseover', function(e){
        $Target.find('.m-stockInfo_balloon').show();
    });

    $Target.bind('mouseout', function(e){
    	$Target.find('.m-stockInfo_balloon').hide();
    });

};
</script>
<script type="text/javascript">
    _nk.stockInfoBallonBaseOver("#JSID_stockInfoHigh");
    _nk.stockInfoBallonBaseOver("#JSID_stockInfoLow");
</script>
</div>

    </div>
</div>

<script type="text/javascript">

_nk.stockInfoAccordion = function (sTargetId) {
    var oNk = _nk;
    var $Target = $(sTargetId);

    // イベント設定
    var $StockInfoButton = $Target.find('.js-stockInfo_btn > a');
    $StockInfoButton.bind('click', function(e){
        e.preventDefault();
        var $btn = $(this).parent('.js-stockInfo_btn');

        $btn.siblings('.js-stockInfo_accordion').slideToggle(200, function(){
            if($btn.hasClass('open')){
                $btn.removeClass('open').addClass('close');
                _nk.putCookie({JSID_dataSetCookie_name:"ap",JSID_dataSetCookie_value:"0",JSID_dataSetCookie_expire:_nk.COOKIE_EXPIRE_LONG});
            }else{
                $btn.removeClass('close').addClass('open');
                _nk.putCookie({JSID_dataSetCookie_name:"ap",JSID_dataSetCookie_value:"1",JSID_dataSetCookie_expire:_nk.COOKIE_EXPIRE_LONG});
            }
        });
    });
};
</script>
<script type="text/javascript">
    _nk.stockInfoAccordion("#JSID_stockInfo");
    _nk.stockInfoAccordionKeep();
    _nk.setMeigaraFolder();
    _nk.setDoublePopup();
</script>

