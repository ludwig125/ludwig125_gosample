	<script>
		$(function(){
		var topBtn = $('.gotopage-top');
		var showFlag = false;
		//スクロールしたらボタン表示
		$(window).scroll(function () {
			if ($(this).scrollTop() > 100) {
				if (showFlag == false) {
					showFlag = true;
					topBtn.css('opacity','');
					topBtn.stop().fadeIn(500).css('display','table');
				}
			} else {
				if (showFlag) {
					showFlag = false;
					topBtn.stop().fadeOut(500,function() {
						topBtn.css('opacity','');
					});
				}
			}
		});
	});
	</script>
